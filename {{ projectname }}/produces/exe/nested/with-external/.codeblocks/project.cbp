<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="{{ project_slug }}" />
		<Option pch_mode="2" />
		<Option default_target="vtgt_exe_{{ exename }}_dbg" />
		<Option compiler="gcc" />
		<Build>
			<Target title="tgt_lib_their_dbg">
				<Option output="../{{ build_directory }}/codeblocks/debug/lib/their" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="1" extension_auto="1" />
				<Option object_output="../{{ build_directory }}/codeblocks/debug/obj" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option projectCompilerOptionsRelation="1" />
				<Option projectLinkerOptionsRelation="1" />
				<Option projectIncludeDirsRelation="1" />
				<Option projectResourceIncludeDirsRelation="1" />
				<Option projectLibDirsRelation="1" />
				<Compiler>
					<Add option="-O0" />
					<Add option="-pedantic" />
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-std=c2x" />
					<Add option="-ggdb" />
					<Add option="-g" />
					<Add directory="../{{ external_directory }}/their/include" />
				</Compiler>
			</Target>
			<Target title="tgt_exe_{{ exename }}_dbg">
				<Option output="../{{ build_directory }}/codeblocks/debug/bin/{{ exename }}" prefix_auto="1" extension_auto="1" />
				<Option object_output="../{{ build_directory }}/codeblocks/debug/obj" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option projectCompilerOptionsRelation="1" />
				<Option projectLinkerOptionsRelation="1" />
				<Option projectIncludeDirsRelation="1" />
				<Option projectResourceIncludeDirsRelation="1" />
				<Option projectLibDirsRelation="1" />
				<Compiler>
					<Add option="-O0" />
					<Add option="-pedantic" />
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-std=c2x" />
					<Add option="-ggdb" />
					<Add option="-g" />
					<Add directory="../{{ external_directory }}/their/include" />
				</Compiler>
				<Linker>
					<Add option="-Wl,-rpath,../build/codeblocks/debug/lib" />
					<Add library="m" />
					<Add library="their" />
					<Add directory="../{{ build_directory }}/codeblocks/debug/lib" />
				</Linker>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="vtgt_exe_{{ exename }}_dbg" targets="tgt_lib_their_dbg;tgt_exe_{{ exename }}_dbg;" />
		</VirtualTargets>
		<Unit filename="../{{ external_directory }}/their/include/their/addition.h">
			<Option target="tgt_lib_their_dbg" />
		</Unit>
		<Unit filename="../{{ external_directory }}/their/include/their/subtraction.h">
			<Option target="tgt_lib_their_dbg" />
		</Unit>
		<Unit filename="../{{ external_directory }}/their/src/their/addition.c">
			<Option compilerVar="CC" />
			<Option target="tgt_lib_their_dbg" />
		</Unit>
		<Unit filename="../{{ external_directory }}/their/src/their/subtraction.c">
			<Option compilerVar="CC" />
			<Option target="tgt_lib_their_dbg" />
		</Unit>
		<Unit filename="../src/{{ exename }}/addition.h">
			<Option target="tgt_exe_{{ exename }}_dbg" />
		</Unit>
		<Unit filename="../src/{{ exename }}/division.h">
			<Option target="tgt_exe_{{ exename }}_dbg" />
		</Unit>
		<Unit filename="../src/{{ exename }}/multiplication.h">
			<Option target="tgt_exe_{{ exename }}_dbg" />
		</Unit>
		<Unit filename="../src/{{ exename }}/subtraction.h">
			<Option target="tgt_exe_{{ exename }}_dbg" />
		</Unit>
		<Unit filename="../src/{{ exename }}/addition.c">
			<Option compilerVar="CC" />
			<Option target="tgt_exe_{{ exename }}_dbg" />
		</Unit>
		<Unit filename="../src/{{ exename }}/division.c">
			<Option compilerVar="CC" />
			<Option target="tgt_exe_{{ exename }}_dbg" />
		</Unit>
		<Unit filename="../src/{{ exename }}/main.c">
			<Option compilerVar="CC" />
			<Option target="tgt_exe_{{ exename }}_dbg" />
		</Unit>
		<Unit filename="../src/{{ exename }}/multiplication.c">
			<Option compilerVar="CC" />
			<Option target="tgt_exe_{{ exename }}_dbg" />
		</Unit>
		<Unit filename="../src/{{ exename }}/subtraction.c">
			<Option compilerVar="CC" />
			<Option target="tgt_exe_{{ exename }}_dbg" />
		</Unit>
		<Extensions />
	</Project>
</CodeBlocks_project_file>
