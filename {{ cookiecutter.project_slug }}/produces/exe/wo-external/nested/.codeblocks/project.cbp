<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="{{ cookiecutter.project_slug }}" />
		<Option pch_mode="2" />
		<Option default_target="debug-exe" />
		<Option compiler="gcc" />
		<Build>
			<Target title="debug-exe">
				<Option output="../{{ cookiecutter.build_directory }}/codeblocks/debug/bin/calculator" prefix_auto="1" extension_auto="1" />
				<Option object_output="../{{ cookiecutter.build_directory }}/codeblocks/debug/obj" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option projectCompilerOptionsRelation="1" />
				<Option projectLinkerOptionsRelation="1" />
				<Option projectIncludeDirsRelation="1" />
				<Option projectResourceIncludeDirsRelation="1" />
				<Option projectLibDirsRelation="1" />
				<Compiler>
					<Add option="-O0" />
					<Add option="-pedantic" />
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-std=c2x" />
					<Add option="-ggdb" />
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add option='-Wl,-rpath,../build/codeblocks/debug/lib' />
					<Add library="m" />
					<Add directory="../{{ cookiecutter.build_directory }}/codeblocks/debug/lib" />
				</Linker>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="debug" targets="debug-exe;" />
		</VirtualTargets>
		<Unit filename="../src/calculator/division.h">
			<Option target="debug-exe" />
		</Unit>
		<Unit filename="../src/calculator//multiplication.h">
			<Option target="debug-exe" />
		</Unit>
		<Unit filename="../src/calculator//division.c">
			<Option compilerVar="CC" />
			<Option target="debug-exe" />
		</Unit>
		<Unit filename="../src/calculator/main.c">
			<Option compilerVar="CC" />
			<Option target="debug-exe" />
		</Unit>
		<Unit filename="../src/calculator//multiplication.c">
			<Option compilerVar="CC" />
			<Option target="debug-exe" />
		</Unit>
		<Extensions />
	</Project>
</CodeBlocks_project_file>
