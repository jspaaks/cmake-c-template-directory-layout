<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="{{ cookiecutter.project_slug }}" />
		<Option pch_mode="2" />
		{%- if cookiecutter.add_test %}
		<Option default_target="vtgt_exe_test_{{ cookiecutter.libname }}_dbg" />
		{%- else %}
		<Option default_target="vtgt_exe_{{ cookiecutter.exename }}_dbg" />
		{%- endif %}
		<Option compiler="gcc" />
		<Build>
			<Target title="tgt_lib_{{ cookiecutter.libname }}_dbg">
				<Option output="../{{ cookiecutter.build_directory }}/codeblocks/debug/lib/{{ cookiecutter.libname }}" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="1" extension_auto="1" />
				<Option object_output="../{{ cookiecutter.build_directory }}/codeblocks/debug/obj" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option projectCompilerOptionsRelation="1" />
				<Option projectLinkerOptionsRelation="1" />
				<Option projectIncludeDirsRelation="1" />
				<Option projectResourceIncludeDirsRelation="1" />
				<Option projectLibDirsRelation="1" />
				<Compiler>
					<Add option="-O0" />
					<Add option="-pedantic" />
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-std=c2x" />
					<Add option="-ggdb" />
					<Add option="-g" />
					<Add directory="../include" />
				</Compiler>
			</Target>
			<Target title="tgt_exe_{{ cookiecutter.exename }}_dbg">
				<Option output="../{{ cookiecutter.build_directory }}/codeblocks/debug/bin/{{ cookiecutter.exename }}" prefix_auto="1" extension_auto="1" />
				<Option object_output="../{{ cookiecutter.build_directory }}/codeblocks/debug/obj" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option projectCompilerOptionsRelation="1" />
				<Option projectLinkerOptionsRelation="1" />
				<Option projectIncludeDirsRelation="1" />
				<Option projectResourceIncludeDirsRelation="1" />
				<Option projectLibDirsRelation="1" />
				<Compiler>
					<Add option="-O0" />
					<Add option="-pedantic" />
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-std=c2x" />
					<Add option="-ggdb" />
					<Add option="-g" />
					<Add directory="../include" />
				</Compiler>
				<Linker>
					<Add option="-Wl,-rpath,../build/codeblocks/debug/lib" />
					<Add library="m" />
					<Add library="{{ cookiecutter.libname }}" />
					<Add directory="../{{ cookiecutter.build_directory }}/codeblocks/debug/lib" />
				</Linker>
			</Target>
			{%- if cookiecutter.add_test %}
			<Target title="tgt_exe_test_{{ cookiecutter.libname }}_dbg">
				<Option output="../{{ cookiecutter.build_directory }}/codeblocks/debug/bin/test_{{ cookiecutter.libname }}" prefix_auto="1" extension_auto="1" />
				<Option object_output="../{{ cookiecutter.build_directory }}/codeblocks/debug/obj" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters="--verbose -j1" />
				<Option projectCompilerOptionsRelation="1" />
				<Option projectLinkerOptionsRelation="1" />
				<Option projectIncludeDirsRelation="1" />
				<Option projectResourceIncludeDirsRelation="1" />
				<Option projectLibDirsRelation="1" />
				<Compiler>
					<Add option="-O0" />
					<Add option="-pedantic" />
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-std=c2x" />
					<Add option="-ggdb" />
					<Add option="-g" />
					<Add directory="../include" />
				</Compiler>
				<Linker>
					<Add option="-Wl,-rpath,../build/codeblocks/debug/lib" />
					<Add library="{{ cookiecutter.libname }}" />
					<Add library="criterion" />
					<Add directory="../{{ cookiecutter.build_directory }}/codeblocks/debug/lib" />
				</Linker>
			</Target>
			{%- endif %}
		</Build>
		<VirtualTargets>
			<Add alias="vtgt_exe_{{ cookiecutter.exename }}_dbg" targets="tgt_lib_their_dbg;tgt_lib_{{ cookiecutter.libname }}_dbg;tgt_exe_{{ cookiecutter.exename }}_dbg;" />
			{%- if cookiecutter.add_test %}
			<Add alias="vtgt_exe_test_{{ cookiecutter.libname }}_dbg" targets="tgt_lib_their_dbg;tgt_lib_{{ cookiecutter.libname }}_dbg;tgt_exe_test_{{ cookiecutter.libname }}_dbg;" />
			{%- endif %}
		</VirtualTargets>
		<Unit filename="../include/{{ cookiecutter.libname }}/division.h">
			<Option target="tgt_lib_{{ cookiecutter.libname }}_dbg" />
		</Unit>
		<Unit filename="../include/{{ cookiecutter.libname }}/multiplication.h">
			<Option target="tgt_lib_{{ cookiecutter.libname }}_dbg" />
		</Unit>
		<Unit filename="../src/{{ cookiecutter.libname }}/division.c">
			<Option compilerVar="CC" />
			<Option target="tgt_lib_{{ cookiecutter.libname }}_dbg" />
		</Unit>
		<Unit filename="../src/{{ cookiecutter.exename }}/main.c">
			<Option compilerVar="CC" />
			<Option target="tgt_exe_{{ cookiecutter.exename }}_dbg" />
		</Unit>
		<Unit filename="../src/{{ cookiecutter.libname }}/multiplication.c">
			<Option compilerVar="CC" />
			<Option target="tgt_lib_{{ cookiecutter.libname }}_dbg" />
		</Unit>
		{%- if cookiecutter.add_test %}
		<Unit filename="../test/{{ cookiecutter.libname }}/test_division.c">
			<Option compilerVar="CC" />
			<Option target="tgt_exe_test_{{ cookiecutter.libname }}_dbg" />
		</Unit>
		<Unit filename="../test/{{ cookiecutter.libname }}/test_multiplication.c">
			<Option compilerVar="CC" />
			<Option target="tgt_exe_test_{{ cookiecutter.libname }}_dbg" />
		</Unit>
		{%- endif %}
		<Extensions />
	</Project>
</CodeBlocks_project_file>
