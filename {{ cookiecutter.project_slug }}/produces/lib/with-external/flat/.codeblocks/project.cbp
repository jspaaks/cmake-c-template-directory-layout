<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="{{ cookiecutter.project_slug }}" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="debug">
				<Option output="../{{ cookiecutter.build_directory }}/codeblocks/debug/bin/{{ cookiecutter.project_slug }}" prefix_auto="1" extension_auto="1" />
				<Option object_output="../{{ cookiecutter.build_directory }}/codeblocks/debug/obj/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O0" />
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="release">
				<Option output="../{{ cookiecutter.build_directory }}/codeblocks/release/bin/{{ cookiecutter.project_slug }}" prefix_auto="1" extension_auto="1" />
				<Option object_output="../{{ cookiecutter.build_directory }}/codeblocks/release/obj/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-pedantic" />
			<Add option="-Wextra" />
			<Add option="-Wall" />
			<Add option="-std=c2x" />
{%- if cookiecutter.external_directory != "My project doesn't have external dependencies" -%}
			<Add directory="../{{ cookiecutter.external_directory }}/addition/include" />
			<Add directory="../{{ cookiecutter.external_directory }}/subtraction/include" />
{%- endif -%}
			<Add directory="../include/lib1" />
		</Compiler>
{%- if cookiecutter.external_directory != "My project doesn't have external dependencies" -%}
		<Unit filename="../{{ cookiecutter.external_directory }}/adding/include/adding.h" />
		<Unit filename="../{{ cookiecutter.external_directory }}/adding/src/adding.c">
		<Unit filename="../{{ cookiecutter.external_directory }}/subtraction/include/adding.h" />
		<Unit filename="../{{ cookiecutter.external_directory }}/subtraction/src/adding.c">
{%- endif -%}
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="../include/lib1/header1.h" />
		<Unit filename="../src/main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions />
	</Project>
</CodeBlocks_project_file>
