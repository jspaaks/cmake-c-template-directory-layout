<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="{{ cookiecutter.project_slug }}" />
		<Option pch_mode="2" />
		<Option default_target="debug-{{ cookiecutter.libname }}" />
		<Option compiler="gcc" />
		<Build>
			<Target title="debug-libtheirs">
				<Option output="../{{ cookiecutter.build_directory }}/codeblocks/debug/lib/libtheirs.so" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="0" extension_auto="0" />
				<Option object_output="../{{ cookiecutter.build_directory }}/codeblocks/debug/obj" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option projectCompilerOptionsRelation="1" />
				<Option projectLinkerOptionsRelation="1" />
				<Option projectIncludeDirsRelation="1" />
				<Option projectResourceIncludeDirsRelation="1" />
				<Option projectLibDirsRelation="1" />
				<Compiler>
					<Add option="-O0" />
					<Add option="-pedantic" />
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-std=c2x" />
					<Add option="-ggdb" />
					<Add option="-g" />
					<Add directory="../{{ cookiecutter.external_directory }}/libtheirs/include" />
				</Compiler>
			</Target>
			<Target title="debug-{{ cookiecutter.libname }}">
				<Option output="../{{ cookiecutter.build_directory }}/codeblocks/debug/lib/{{ cookiecutter.libname }}.so" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="0" extension_auto="0" />
				<Option object_output="../{{ cookiecutter.build_directory }}/codeblocks/debug/obj" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option projectCompilerOptionsRelation="1" />
				<Option projectLinkerOptionsRelation="1" />
				<Option projectIncludeDirsRelation="1" />
				<Option projectResourceIncludeDirsRelation="1" />
				<Option projectLibDirsRelation="1" />
				<Compiler>
					<Add option="-O0" />
					<Add option="-pedantic" />
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-std=c2x" />
					<Add option="-ggdb" />
					<Add option="-g" />
					<Add directory="../include" />
					<Add directory="../{{ cookiecutter.external_directory }}/libtheirs/include" />
				</Compiler>
				<Linker>
					<Add library="theirs" />
					<Add directory="../{{ cookiecutter.build_directory }}/codeblocks/debug/lib" />
				</Linker>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="debug" targets="debug-libtheirs;debug-{{ cookiecutter.libname }};" />
		</VirtualTargets>
		<Unit filename="../{{ cookiecutter.external_directory }}/libtheirs/include/their_addition.h">
			<Option target="debug-libtheirs" />
		</Unit>
		<Unit filename="../{{ cookiecutter.external_directory }}/libtheirs/include/their_subtraction.h">
			<Option target="debug-libtheirs" />
		</Unit>
		<Unit filename="../{{ cookiecutter.external_directory }}/libtheirs/src/their_addition.c">
			<Option compilerVar="CC" />
			<Option target="debug-libtheirs" />
		</Unit>
		<Unit filename="../{{ cookiecutter.external_directory }}/libtheirs/src/their_subtraction.c">
			<Option compilerVar="CC" />
			<Option target="debug-libtheirs" />
		</Unit>
		<Unit filename="../include/addition.h">
			<Option target="debug-{{ cookiecutter.libname }}" />
		</Unit>
		<Unit filename="../include/division.h">
			<Option target="debug-{{ cookiecutter.libname }}" />
		</Unit>
		<Unit filename="../include/multiplication.h">
			<Option target="debug-{{ cookiecutter.libname }}" />
		</Unit>
		<Unit filename="../include/subtraction.h">
			<Option target="debug-{{ cookiecutter.libname }}" />
		</Unit>
		<Unit filename="../src/addition.c">
			<Option compilerVar="CC" />
			<Option target="debug-{{ cookiecutter.libname }}" />
		</Unit>
		<Unit filename="../src/division.c">
			<Option compilerVar="CC" />
			<Option target="debug-{{ cookiecutter.libname }}" />
		</Unit>
		<Unit filename="../src/multiplication.c">
			<Option compilerVar="CC" />
			<Option target="debug-{{ cookiecutter.libname }}" />
		</Unit>
		<Unit filename="../src/subtraction.c">
			<Option compilerVar="CC" />
			<Option target="debug-{{ cookiecutter.libname }}" />
		</Unit>
		<Extensions />
	</Project>
</CodeBlocks_project_file>

