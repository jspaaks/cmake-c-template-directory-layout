<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="{{ projectname }}" />
		<Option pch_mode="2" />
		{%- if add_test %}
		<Option default_target="vtgt_exe_test_{{ libname }}_dbg" />
		{%- else %}
		<Option default_target="vtgt_exe_{{ exename }}_dbg" />
		{%- endif %}
		<Option compiler="gcc" />
		<Build>
			<Target title="tgt_lib_{{ libname }}_dbg">
				<Option output="../{{ build_directory }}/codeblocks/debug/lib/{{ libname }}" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="1" extension_auto="1" />
				<Option object_output="../{{ build_directory }}/codeblocks/debug/obj" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option projectCompilerOptionsRelation="1" />
				<Option projectLinkerOptionsRelation="1" />
				<Option projectIncludeDirsRelation="1" />
				<Option projectResourceIncludeDirsRelation="1" />
				<Option projectLibDirsRelation="1" />
				<Compiler>
					<Add option="-O0" />
					<Add option="-pedantic" />
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-std=c2x" />
					<Add option="-ggdb" />
					<Add option="-g" />
					<Add directory="../include" />
				</Compiler>
			</Target>
			<Target title="tgt_exe_{{ exename }}_dbg">
				<Option output="../{{ build_directory }}/codeblocks/debug/bin/{{ exename }}" prefix_auto="1" extension_auto="1" />
				<Option object_output="../{{ build_directory }}/codeblocks/debug/obj" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option projectCompilerOptionsRelation="1" />
				<Option projectLinkerOptionsRelation="1" />
				<Option projectIncludeDirsRelation="1" />
				<Option projectResourceIncludeDirsRelation="1" />
				<Option projectLibDirsRelation="1" />
				<Compiler>
					<Add option="-O0" />
					<Add option="-pedantic" />
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-std=c2x" />
					<Add option="-ggdb" />
					<Add option="-g" />
					<Add directory="../include" />
				</Compiler>
				<Linker>
					<Add option="-Wl,-rpath,../{{ build_directory }}/codeblocks/debug/lib" />
					<Add library="m" />
					<Add library="{{ libname }}" />
					<Add directory="../{{ build_directory }}/codeblocks/debug/lib" />
				</Linker>
			</Target>
			{%- if add_test %}
			<Target title="tgt_exe_test_{{ libname }}_dbg">
				<Option output="../{{ build_directory }}/codeblocks/debug/bin/test_{{ libname }}" prefix_auto="1" extension_auto="1" />
				<Option object_output="../{{ build_directory }}/codeblocks/debug/obj" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters="--verbose -j1" />
				<Option projectCompilerOptionsRelation="1" />
				<Option projectLinkerOptionsRelation="1" />
				<Option projectIncludeDirsRelation="1" />
				<Option projectResourceIncludeDirsRelation="1" />
				<Option projectLibDirsRelation="1" />
				<Compiler>
					<Add option="-O0" />
					<Add option="-pedantic" />
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-std=c2x" />
					<Add option="-ggdb" />
					<Add option="-g" />
					<Add directory="../include" />
				</Compiler>
				<Linker>
					<Add option="-Wl,-rpath,../{{ build_directory }}/codeblocks/debug/lib" />
					<Add library="{{ libname }}" />
					<Add library="criterion" />
					<Add directory="../{{ build_directory }}/codeblocks/debug/lib" />
				</Linker>
			</Target>
			{%- endif %}
		</Build>
		<VirtualTargets>
			<Add alias="vtgt_exe_{{ exename }}_dbg" targets="tgt_lib_{{ libname }}_dbg;tgt_exe_{{ exename }}_dbg;" />
			{%- if add_test %}
			<Add alias="vtgt_exe_test_{{ libname }}_dbg" targets="tgt_lib_{{ libname }}_dbg;tgt_exe_test_{{ libname }}_dbg;" />
			{%- endif %}
		</VirtualTargets>
		<Unit filename="../include/division.h">
			<Option target="tgt_lib_{{ libname }}_dbg" />
		</Unit>
		<Unit filename="../include/multiplication.h">
			<Option target="tgt_lib_{{ libname }}_dbg" />
		</Unit>
		<Unit filename="../src/division.c">
			<Option compilerVar="CC" />
			<Option target="tgt_lib_{{ libname }}_dbg" />
		</Unit>
		<Unit filename="../src/main.c">
			<Option compilerVar="CC" />
			<Option target="tgt_exe_{{ exename }}_dbg" />
		</Unit>
		<Unit filename="../src/multiplication.c">
			<Option compilerVar="CC" />
			<Option target="tgt_lib_{{ libname }}_dbg" />
		</Unit>
		{%- if add_test %}
		<Unit filename="../test/test_division.c">
			<Option compilerVar="CC" />
			<Option target="tgt_exe_test_{{ libname }}_dbg" />
		</Unit>
		<Unit filename="../test/test_multiplication.c">
			<Option compilerVar="CC" />
			<Option target="tgt_exe_test_{{ libname }}_dbg" />
		</Unit>
		{%- endif %}
		<Extensions />
	</Project>
</CodeBlocks_project_file>
